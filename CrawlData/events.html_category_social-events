// Have Button component instances with appropriate class names and display:none;

$(document).ready(function() {
  var category = getParameterByName("category");

  if (category == "information-orientation/concordia-in-your-town") {
    $(".concordia-in-your-town").fadeIn("slow");
  }
  else if (category == "information-orientation/tours") {
    $(".tours").fadeIn("slow");
  }
});

function getParameterByName(name, url) {
    if (!url) url = window.location.href;
    name = name.replace(/[\[\]]/g, "\\$&");
    var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
        results = regex.exec(url);
    if (!results) return null;
    if (!results[2]) return '';
    return decodeevents/submit-an-event.htmlcript></div>
