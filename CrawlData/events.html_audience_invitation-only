	<div class="c-sidebar-menu bloc category-filter">
        <script>
            jQuery(function($) {
                if ($("ul#event-audiences ul.level0 li").hasClass("open")) {
					$("#event-audiences ul.level0 li.open").children("ul:first").slideToggle("fast");
                    $("#event-audiences ul.level0 li.open .arrow").addClass("open");
                }

				else if ($("ul#event-audiences ul.level1 li a").hasClass("active")) {
					$("#event-audiences ul.level1 li a.active").parent().parent().slideToggle("fast");
                    $("#event-audiences ul.level1 li a.active").parent().parent().parent().addClass("open");
                    $("#event-audiences ul.level1 li a.active").parent().parent().parent().children(".arrow").addClass("open");
                }

                $("ul#event-audiences .arrow").bind('click touch', function(e) {
                    var $this = $(this);

                    // Put a little timer so the closing level doesn't change background color
                    if ($this.parent().hasClass('open')) {
                        setTimeout(function(){$this.parent().toggleClass('open');}, 200);
                    } else {
                        $this.parent().toggleClass('open');
                    }
                });
            });

            function getURLParameter(name) {
                return decodeURIComponent((new RegExp('[?|&]' + name + '=' + '([^&;]+?)(&|#|;|$)').exec(location.search)||[,""])[1].replace(/\+/g, '%20'))||null;
            }
        </script>

		

