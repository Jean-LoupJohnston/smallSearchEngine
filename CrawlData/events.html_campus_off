    <div class="c-sidebar-menu bloc category-filter">events.html_campus_off          jQuery(function($) {
                if ($("ul#concordia-events-categories ul.level0 li").hasClass("open")) {
					$("#concordia-events-categories ul.level0 li.open").children("ul:first").slideToggle("fast");
                    $("#concordia-events-categories ul.level0 li.open .arrow").addClass("open");
                }

				else if ($("ul#concordia-events-categories ul.level1 li a").hasClass("active")) {
					$("#concordia-events-categories ul.level1 li a.active").parent().parent().slideToggle("fast");
                    $("#concordia-events-categories ul.level1 li a.active").parent().parent().parent().addClass("open");
                    $("#concordia-events-categories ul.level1 li a.active").parent().parent().parent().children(".arrow").addClass("open");
                }

                $("ul#concorevents.html_campus_all.arrow").bind('click touch', function(e) {
                    var $this = $(this);

                    // Put a little timer so the closing level doesn't cevents.html_campus_sgw
                    if ($this.parent().hasClass('open')) {
                        setTimeout(function(){$this.parent().toggleClass('open');}, 200);
              events.html_campus_loy                  $this.parent().toggleClass('open');
                    }
                });
            });

            function getURLParameterevents.html_campus_off    return decodeURIComponent((new RegExp('[?|&]' + name + '=' + '([^&;]+?)(&|#|;|$)').exec(location.search)||[,""])[1].replace(/\+/g, '%20'))||null;
            }
        </script>

        

